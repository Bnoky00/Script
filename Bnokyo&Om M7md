	Name = "Super Fast [BETA]",
	Callback = function()
      		Walkspeed = 85
      game.Players.LocalPlayer.Character. Humanoid.WalkSpeed = Walkspeed
      OrionLib:MakeNotification({ 	Name = "Done", 	Content = "SuperFast ON", 	Image = "rbxassetid://17330343750", 	Time = 3 })
  	end    
})

--[[
Name = <string> - The name of the button.
